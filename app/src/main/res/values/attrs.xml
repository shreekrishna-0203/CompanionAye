<resources>
    <!-- Attributes for ReelActionButton -->
    <declare-styleable name="ReelActionButton">
        <attr name="buttonText" format="string" />
        <attr name="buttonIcon" format="reference" />
    </declare-styleable>

    <!-- Attributes for ReelNavButton -->
    <declare-styleable name="ReelNavButton">
        <attr name="buttonText2" format="string" />
        <attr name="buttonIcon2" format="reference" />
        <attr name="iconPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>
